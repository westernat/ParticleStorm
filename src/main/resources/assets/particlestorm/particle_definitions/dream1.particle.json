{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "particlestorm:dream1",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "minecraft:glitter_7"
			}
		},
		"components": {
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 100,
				"max_particles": 1000
			},
			"minecraft:emitter_lifetime_expression": {},
			"minecraft:emitter_shape_point": {
				"offset": ["math.cos(variable.s)", 0, "math.sin(variable.s)"],
				"direction": [0, -1, 0]
			},
			"minecraft:particle_initialization": {
				"per_render_expression": "variable.t = variable.particle_age / variable.particle_lifetime;variable.s = variable.particle_random_1 * 360;variable.k = math.clamp(variable.t*1.3,0,1);"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 10
			},
			"minecraft:particle_initial_spin": {
				"rotation_rate": -60
			},
			"minecraft:particle_initial_speed": 2,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["-math.sin(variable.s) * 0.4", 0, "math.cos(variable.s) * 0.4"],
				"linear_drag_coefficient": 0.4,
				"rotation_acceleration": -10
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["math.lerp(0.2, 0.7, variable.k)", "math.lerp(0.2, 0.7, variable.k)"],
				"facing_camera_mode": "rotate_xyz"
			},
			"minecraft:particle_appearance_tinting": {
				"color": ["math.lerp(1, 0.2, variable.k)", "math.lerp(variable.particle_random_2 * 0.4 + 0.2, 0.2, variable.k)", "math.lerp(0, 0.2, variable.k)", "math.clamp(1-variable.t,0.2,0.8)+0.2"]
			},
			"minecraft:particle_motion_collision": {}
		}
	}
}